{
  "hostname": {
    "production": "https://www.edulib.fr",
    "staging": "https://staging-edulib.xair.cloud"
  },
  "basePath": "/api",
  "authenticate": {
    "method": "POST",
    "path": "/oauth/token"
  },
  "versions": {
    "v2": {
      "webServices": {
        "getUser": { "path": "/users/me" },
        "getUserLicenses": { "path": "/users/licenses" },
        "getUserClassrooms": { "path": "/users/classrooms" }
      }
    },
    "v3": {
      "webServices": {
        "getUserBySsoId": { "path": "/users/identify-by-sso" },
        "getUserByCredential": { "path": "/users/identify-by-credentials" },
        "setUserSsoId": { "method": "PATCH", "path": "/users/set-sso-id" },
        "getEtablishmentCatalog": { "path": "/establishment_accounts/catalog" }
      }
    }
  }
}